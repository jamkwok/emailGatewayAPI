extends _bootstrap
append styles
        // Custom styles for this template
        link(href='./css/starter-template.css', rel='stylesheet')
block body
        +navbar-fixed("Argus Hometrack Monitoring","navbar","inverse")
                +nav_item("#","active") #{env}
        .container
                .starter-template
                        br
                        p.lead
                                |  Available endpoints:.
                                each item in JSON.parse(check)
                                      if item.status == "Error"
                                            +alert-i-danger(item.uri + ' => ' + item.status)
                                      else
                                            +alert-i-success(item.uri + ' => ' + item.status)
